{
  "pages": [
    "pages/index/index",
    "pages/paper/index",
    "pages/paper/before/before",
    "pages/circle/index",
    "pages/circle/add/add",
    "pages/hub/index",
    "pages/me/index",
    "pages/me/setting/setting",
    "pages/me/setting/theme/theme"
  ],
  "window": {
    "navigationBarBackgroundColor": "@navigationBarBackgroundColor",
    "navigationBarTextStyle": "@navigationBarTextStyle",
    "enablePullDownRefresh": true,
    "backgroundColor": "@backgroundColor",
    "backgroundTextStyle": "@backgroundTextStyle",
    "backgroundColorTop": "@backgroundColorTop",
    "backgroundColorBottom": "backgroundColorBottom"
  },
  "tabBar": {
    "color": "@color",
    "selectedColor": "#07c160",
    "borderStyle": "@borderStyle",
    "backgroundColor": "@tabBarBackgroundColor",
    "list": [{
        "pagePath": "pages/paper/before/before",
        "iconPath": "images/icon_tabbar.png",
        "selectedIconPath": "images/icon_tabbar.png",
        "text": "Demo"
      },
      {
        "pagePath": "pages/circle/index",
        "iconPath": "images/icon_tabbar.png",
        "selectedIconPath": "images/icon_tabbar.png",
        "text": "圈子"
      },
      {
        "pagePath": "pages/hub/index",
        "iconPath": "images/icon_tabbar.png",
        "selectedIconPath": "images/icon_tabbar.png",
        "text": "题库"
      },
      {
        "pagePath": "pages/me/index",
        "iconPath": "images/icon_tabbar.png",
        "selectedIconPath": "images/icon_tabbar.png",
        "text": "我"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "debug": true,
  "darkmode": true,
  "themeLocation": "style/theme.json",
  "sitemapLocation": "sitemap.json",
  "style": "v2",
  "useExtendedLib": {
    "weui": true
  }
}